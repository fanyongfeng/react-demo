{
  "name": "react-table",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development DEPLOYMENT_ENV=stage node scripts/start.js",
    "build-qa": "cross-env NODE_ENV=production DEPLOYMENT_ENV=qa node scripts/build.js",
    "build-stage": "cross-env NODE_ENV=production DEPLOYMENT_ENV=stage node scripts/build.js",
    "build": "cross-env NODE_ENV=production DEPLOYMENT_ENV=prod node scripts/build.js",
    "stylelint:cs": "cross-env stylelint 'src/**/*.css' --fix",
    "stylelint:scs": "cross-env stylelint 'src/**/*.scss'--syntax scss",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "antd": "^3.2.2",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "dotenv": "4.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "^4.17.5",
    "match-sorter": "^2.0.2",
    "md5": "^2.2.1",
    "namor": "^1.0.1",
    "object-assign": "4.1.1",
    "paper": "^0.11.5",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.1.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.7.4",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-thunk": "^2.2.0",
    "store2": "^2.6.0",
    "video.js": "^6.7.3",
    "videojs-flash": "^2.1.0",
    "whatwg-fetch": "2.0.3",
    "zrender": "^4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "6.26.0",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "css-loader": "0.28.7",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-react-tools": "^1.0.10",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "stylelint": "^8.3.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "video.js": "^6.4.0",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "proxy": {
    "/tmops/api": {
      "target": "https://internal-stage.tutormeetplus.com",
      "changeOrigin": true
    }
  }
}
